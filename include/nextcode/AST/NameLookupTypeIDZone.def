NEXTCODE_REQUEST(NameLookup, LookupPostfixOperatorRequest,
              PostfixOperatorDecl *(OperatorLookupDescriptor),
              Cached, NoLocationInfo)
NEXTCODE_REQUEST(NameLookup, LookupPrecedenceGroupRequest,
              TinyPtrVector<PrecedenceGroupDecl *>(OperatorLookupDescriptor),
