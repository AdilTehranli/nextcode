              Uncached, NoLocationInfo)
NEXTCODE_REQUEST(NameLookup, SelfBoundsFromGenericSignatureRequest,
              SelfBounds(const ExtensionDecl * extDecl),
              Uncached, NoLocationInfo)
NEXTCODE_REQUEST(NameLookup, SuperclassDeclRequest, ClassDecl *(NominalTypeDecl *),
