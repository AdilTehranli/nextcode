NEXTCODE_REQUEST(NameLookup, HasMissingDesignatedInitializersRequest,
              bool(ClassDecl *),
              SeparatelyCached, NoLocationInfo)
NEXTCODE_REQUEST(NameLookup, TypeDeclsFromWhereClauseRequest,
              DirectlyReferencedTypeDecls(ExtensionDecl *), Uncached,
              NoLocationInfo)
NEXTCODE_REQUEST(NameLookup, UnderlyingTypeDeclsReferencedRequest,
              DirectlyReferencedTypeDecls(TypeAliasDecl *), Uncached,
              NoLocationInfo)
NEXTCODE_REQUEST(NameLookup, UnqualifiedLookupRequest,
              LookupResult(UnqualifiedLookupDescriptor), Uncached,
              NoLocationInfo)

NEXTCODE_REQUEST(NameLookup, LookupPrefixOperatorRequest,
              PrefixOperatorDecl *(OperatorLookupDescriptor),
              Cached, NoLocationInfo)
NEXTCODE_REQUEST(NameLookup, LookupInfixOperatorRequest,
              TinyPtrVector<InfixOperatorDecl *>(OperatorLookupDescriptor),
