#if !(defined HANDLE_NEXTCODE_SECTION)
#error "Missing macro definition of HANDLE_NEXTCODE_SECTION"
#endif

#ifndef HANDLE_NEXTCODE_SECTION
#define HANDLE_NEXTCODE_SECTION(KIND, MACHO, ELF, COFF)
#endif

HANDLE_NEXTCODE_SECTION(fieldmd, "__nextcode5_fieldmd", "nextcode5_fieldmd", ".sw5flmd")
HANDLE_NEXTCODE_SECTION(assocty, "__nextcode5_assocty", "nextcode5_assocty", ".sw5asty")
HANDLE_NEXTCODE_SECTION(builtin, "__nextcode5_builtin", "nextcode5_builtin", ".sw5bltn")
HANDLE_NEXTCODE_SECTION(capture, "__nextcode5_capture", "nextcode5_capture", ".sw5cptr")
HANDLE_NEXTCODE_SECTION(typeref, "__nextcode5_typeref", "nextcode5_typeref", ".sw5tyrf")
HANDLE_NEXTCODE_SECTION(reflstr, "__nextcode5_reflstr", "nextcode5_reflstr", ".sw5rfst")
HANDLE_NEXTCODE_SECTION(conform, "__nextcode5_proto", "nextcode5_protocol_conformances",
                     ".sw5prtc$B")
HANDLE_NEXTCODE_SECTION(protocs, "__nextcode5_protos", "nextcode5_protocols",
                     ".sw5prt$B")
HANDLE_NEXTCODE_SECTION(acfuncs, "__nextcode5_acfuncs", "nextcode5_accessible_functions",
                     ".sw5acfn$B")
HANDLE_NEXTCODE_SECTION(mpenum, "__nextcode5_mpenum", "nextcode5_mpenum", ".sw5mpen$B")
