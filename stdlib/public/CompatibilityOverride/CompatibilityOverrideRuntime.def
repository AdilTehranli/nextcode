#    define OVERRIDE_METADATALOOKUP(...)
#  endif
#  ifndef OVERRIDE_CASTING
#    define OVERRIDE_CASTING(...)
#  endif
#  ifndef OVERRIDE_DYNAMICCASTING
#    define OVERRIDE_DYNAMICCASTING(...)
#  endif
#  ifndef OVERRIDE_OBJC
#    define OVERRIDE_OBJC(...)
#  endif
#  ifndef OVERRIDE_FOREIGN
